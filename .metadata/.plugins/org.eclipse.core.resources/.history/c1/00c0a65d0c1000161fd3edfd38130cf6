package com.buaa.stock;

import org.apache.hadoop.hive.ql.exec.UDF;

import sun.org.mozilla.javascript.internal.EvaluatorException;

public class StockMax extends UDF{

	public Double evaluate(Double a,Double b){
		if(a == null){
			a = 0.0;
		}
		
		if(b == null){
			b = 0.0;
		}
		
		if(a > b){
			return a;
		}
		
		if(a < b){
			return b;
		}
		return b;
	}
}
